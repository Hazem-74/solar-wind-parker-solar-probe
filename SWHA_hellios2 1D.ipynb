    bikappa,
    v_arr,
    f_arr,
    p0=[
        7.07424756e+02, 9.76605403e+02,     # u1, u2
        2.39334027e-01, 3.01375408e+03,     # T1, T2
        4.46333977e-11, 5.68968931e-12,     # n1, n2
