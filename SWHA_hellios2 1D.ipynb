display(Math(r'\frac{\sigma_\alpha}{\sigma_p} = %.3f' % sigma_ratio))
display(Math(r'\frac{v_{\text{th}, \alpha}}{v_{\text{th}, p}} = %.3f' % thermal_velocity_ratio))
