entropy_pp_ratio = entropy_per_particle_a / entropy_per_particle_p
display(Math(r'\frac{S_\alpha}{S_p} = %.3f' % entropy_ratio))
display(Math(r'\left( \dfrac{S_{\alpha} / n_{\alpha}}{S_{p} / n_{p}} \right) = %.3f' % entropy_pp_ratio))

